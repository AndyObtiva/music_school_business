<div id="<%= dom_id music_class %>">
  <p>
    <strong>Session:</strong>
    <%= music_class.session_id %>
  </p>

  <p>
    <strong>Instructor:</strong>
    <%= music_class.instructor_id %>
  </p>

  <p>
    <strong>Location:</strong>
    <%= music_class.location_id %>
  </p>

  <p>
    <strong>Music class series:</strong>
    <%= music_class.music_class_series_id %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= music_class.name %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= music_class.description %>
  </p>

  <p>
    <strong>Level:</strong>
    <%= music_class.level %>
  </p>

  <p>
    <strong>Day:</strong>
    <%= music_class.day %>
  </p>

  <p>
    <strong>Lesson count:</strong>
    <%= music_class.lesson_count %>
  </p>

  <p>
    <strong>Duration:</strong>
    <%= music_class.duration %>
  </p>

  <p>
    <strong>Price:</strong>
    <%= music_class.price %>
  </p>

</div>
